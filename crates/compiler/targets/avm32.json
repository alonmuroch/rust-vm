{
  "arch": "riscv32",
  "llvm-target": "riscv32",
  "llvm-abiname": "ilp32",
  "vendor": "unknown",
  "os": "none",
  "env": "",
  "target-endian": "little",
  "target-pointer-width": 32,
  "data-layout": "e-m:e-p:32:32-i64:64-n32-S128",
  "cpu": "generic-rv32",
  "features": "+m,+c",
  "max-atomic-width": 0,
  "atomic-cas": false,
  "singlethread": true,
  "panic-strategy": "abort",
  "relocation-model": "static",
  "code-model": "small",
  "executables": true,
  "disable-redzone": true,
  "linker": "rust-lld",
  "linker-flavor": "gnu-lld",
  "linker-is-gnu": true,
  "emit-debug-gdb-scripts": false,
  "pre-link-args": {
    "ld.lld": [
      "--nmagic",
      "--no-relax"
    ]
  },
  "metadata": {
    "description": "AVM single-threaded RV32IMC (no atomics)",
    "host_tools": false,
    "std": false,
    "tier": 3
  }
}
